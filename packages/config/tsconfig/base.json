{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "strict": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "declaration": true,
    "sourceMap": true,
    "target": "es6",
    "composite": true,
    "skipLibCheck": true
  }
}
